<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.keiwando.lib_nativefileso">

    <application>
        <activity android:name="com.keiwando.lib_nativefileso.NativeFileOpenActivity"/>
        <provider
                android:name="com.keiwando.lib_nativefileso.androidx.core.content.FileProvider"
                android:authorities="com.keiwando.nativefileso.provider"
                android:exported="false"
                android:grantUriPermissions="true">
            <meta-data
                    android:name="android.support.FILE_PROVIDER_PATHS"
